{
  "name": "flickr-to-google-cli",
  "version": "1.0.0",
  "description": "A CLI tool for transferring photo albums from Flickr to Google Photos while preserving metadata",
  "main": "dist/index.js",
  "bin": {
    "flickr-to-google": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "node jest-wrapper",
    "test:watch": "node jest-wrapper --watch",
    "test:coverage": "node jest-wrapper --coverage",
    "test:unit": "node jest-wrapper --testPathPattern=unit",
    "test:integration": "node jest-wrapper --testPathPattern=integration",
    "check": "npm run lint && npm run format:check && npm run build && npm run test"
  },
  "keywords": [
    "flickr",
    "google-photos",
    "photo-transfer",
    "cli",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hapi/hapi": "^21.3.0",
    "@hapi/inert": "^7.1.0",
    "@inquirer/prompts": "^4.0.0",
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "commander": "^14.0.1",
    "dotenv": "^16.3.1",
    "flickr-sdk": "^7.1.0",
    "fs-extra": "^11.2.0",
    "googleapis": "^162.0.0",
    "handlebars": "^4.7.8",
    "mime-types": "^2.1.35",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.8",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.19.19",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "<5.4.2"
  },
  "engines": {
    "node": ">=20.16.0"
  }
}
